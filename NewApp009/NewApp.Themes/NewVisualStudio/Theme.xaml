                                        </LinearGradientBrush>
                                    </Border.OpacityMask>
                                </Border>
                                <Border Background="{StaticResource ControlBackgroundBrush}" CornerRadius="0,2.5,0,0" Grid.Column="2">
                                    <Border.OpacityMask>
                                        <RadialGradientBrush GradientOrigin="0.283,0.71">
