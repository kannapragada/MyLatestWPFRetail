<UserControl x:Class="NewApp.ExcelInterOp.UserControls.ExcelInterOpCtrl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" >
    <Grid Height="275" Width="540" Margin="5">
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Margin="5">
            <ListBox Name="lb_Sequence" Height="200" Width="500" Margin="5"></ListBox>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5">
                <Button Name="btn_ExportToExcel" Content="Export To Excel" Click="btn_ExportToExcel_Click" Height="35" Width="125" HorizontalAlignment="Right" Margin="5" Visibility="Collapsed"></Button>
                <Button Name="btn_UploadtoDb" Content="Upload to Database" Click="btn_UploadtoDb_Click" Height="35" Width="125" HorizontalAlignment="Right" Margin="5"></Button>
                <Button Name="btn_Close" Content="Close" Click="btn_Close_Click" Height="35" Width="125" HorizontalAlignment="Right" Margin="5"></Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
